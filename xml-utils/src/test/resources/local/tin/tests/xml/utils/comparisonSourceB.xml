<root xmlns:b="http://a.b.com">
    <!-- isSameNodeShallowly -->    
    <b:nodeA>
        <b:nodeAA a="b">
            Node A value A
        </b:nodeAA>
        <b:nodeAA a="c">
            Node A value A
        </b:nodeAA>
    </b:nodeA>    
    <!-- isSameNodeDeeply -->
    <!-- EQuals -->
    <b:nodeB>
        <b:nodeAA a="b">
            Node A value A
        </b:nodeAA>
        <b:nodeAA a="c">
            Node A value B
        </b:nodeAA>        
    </b:nodeB> 
    <!-- Differenc content -->
    <b:nodeB>
        <b:nodeAA a="b">
            Node A value AX
        </b:nodeAA>
        <b:nodeAA a="c">
            Node A value BX
        </b:nodeAA>        
    </b:nodeB> 
    <!-- isSameNodeList equals -->    
    <b:nodeC>
        <b:nodeCA a="b">
            Node A value A
        </b:nodeCA>
        <b:nodeCA a="c">
            Node A value B
        </b:nodeCA>
    </b:nodeC>      
    <!-- isSameNodeList different number of nodes -->    
    <b:nodeD>
        <b:nodeDA a="b">
            Node A value A
        </b:nodeDA>
        <b:nodeDA a="c">
            Node A value B
        </b:nodeDA>
    </b:nodeD>    
    <!-- isSameNodeList different content -->    
    <b:nodeE>
        <b:nodeEA a="b">
            Node A value A
        </b:nodeEA>
        <b:nodeEA a="d">
            Node A value B
        </b:nodeEA>
    </b:nodeE>  
    <!-- isSameNodeShallowly ignores namespace attributes -->  
    <b:nodeF a="b" xmlns:d="http://c.d.com">
        Node A value B
    </b:nodeF>   
    <!-- isSameNodeShallowly ignores attributes order-->  
    <b:nodeG a="b" c="c">
        Node A value B
    </b:nodeG>                          
</root>
